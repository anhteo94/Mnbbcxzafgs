TabChung:AddButton({
    Title = "D·ªãch Chuy·ªÉn ƒê·∫øn NPC G·∫ßn Nh·∫•t",
    Description = "Auto d·ªãch chuy·ªÉn ƒë·∫øn mob g·∫ßn nh·∫•t",
    Callback = function()
        getgenv().AutoTPMob = true
        while getgenv().AutoTPMob and wait(1) do
            pcall(function()
                local plr = game.Players.LocalPlayer
                local root = plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
                local closest, dist = nil, math.huge
                for _, mob in pairs(workspace.Enemies:GetChildren()) do
                    if mob:FindFirstChild("HumanoidRootPart") and mob:FindFirstChild("Humanoid") and mob.Humanoid.Health > 0 then
                        local mag = (root.Position - mob.HumanoidRootPart.Position).Magnitude
                        if mag < dist then
                            closest = mob
                            dist = mag
                        end
                    end
                end
                if closest then
                    root.CFrame = closest.HumanoidRootPart.CFrame * CFrame.new(0, 0, 5)
                end
            end)
        end
    end
})
