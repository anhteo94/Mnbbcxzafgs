TabCaiDat:AddToggle({
    Title = "T·∫Øt Nh·∫°c N·ªÅn",
    Description = "T·∫Øt to√†n b·ªô √¢m nh·∫°c trong game",
    Default = false,
    Callback = function(state)
        if state then
            for _, s in pairs(game:GetDescendants()) do
                if s:IsA("Sound") and s.Name == "BGM" then
                    s:Stop()
                end
            end
        end
    end
})
TabCaiDat:AddToggle({
    Title = "T·∫Øt B√≥ng ƒê·ªï",
    Description = "TƒÉng FPS b·∫±ng c√°ch t·∫Øt b√≥ng ƒë·ªï",
    Default = false,
    Callback = function(state)
        if state then
            game:GetService("Lighting").GlobalShadows = false
        else
            game:GetService("Lighting").GlobalShadows = true
        end
    end
})
TabCaiDat:AddToggle({
    Title = "Gi·∫£m ƒê·ªô S√°ng",
    Description = "H·∫° ƒë·ªô s√°ng ƒë·ªÉ d·ªÖ nh√¨n v√† ƒë·ª° ch√≥i",
    Default = false,
    Callback = function(state)
